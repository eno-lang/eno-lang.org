{
  "author": "Simon Repp <simon@fdpl.io>",
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "ace-eno": "github:eno-lang/ace-eno",
    "enolib": "^0.8.2",
    "enotype": "^0.3.0",
    "fast-glob": "^3.2.2",
    "fs-extra": "^9.0.0",
    "http-server": "^0.12.3",
    "markdown-it": "^11.0.0",
    "markdown-it-prism": "^2.0.5",
    "moment": "^2.26.0",
    "nodemon": "^2.0.4",
    "parcel-bundler": "^1.12.4",
    "prismjs": "^1.19.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.26.5",
    "speakingurl": "^14.0.1"
  },
  "description": "The eno-lang.org website",
  "name": "eno-lang.org",
  "private": true,
  "repository": "https://github.com/eno-lang/eno-lang.org",
  "scripts": {
    "build": "node build.js && npm run styles && npm run language-demos && npm run libraries-demos",
    "deploy": "node deploy.js",
    "language-demos": "parcel build src/demos/eno.js --no-cache --no-source-maps --out-dir public/eno/demos --out-file bundle.js",
    "libraries-demos": "parcel build src/demos/enolib.js --no-cache --no-source-maps --out-dir public/enolib/demos --out-file bundle.js",
    "release": "npm run build && npm run deploy",
    "serve": "http-server -c-1",
    "styles": "sass --style compressed styles/main.scss public/styles.css",
    "watch": "nodemon --ext css,eno,js,scss --ignore public --exec 'npm run build && npm run serve'"
  }
}
