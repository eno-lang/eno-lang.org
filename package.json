{
  "author": "Simon Repp <simon@fdpl.io>",
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "ace-builds": "^1.4.2",
    "babel-loader": "^8.0.4",
    "enojs": "^0.16.1",
    "fast-glob": "^2.2.4",
    "file-loader": "^3.0.1",
    "fs-extra": "^7.0.1",
    "http-server": "^0.11.1",
    "markdown-it": "^8.4.2",
    "markdown-it-prism": "^2.0.1",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "prismjs": "^1.15.0",
    "speakingurl": "^14.0.1",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.1.2"
  },
  "description": "The eno-lang.org website",
  "name": "eno-lang.org",
  "repository": "https://github.com/eno-lang/eno-lang.org",
  "scripts": {
    "build": "node build.js && npm run styles && webpack",
    "deploy": "node deploy.js",
    "release": "npm run build && npm run deploy",
    "serve": "http-server -c-1",
    "styles": "node-sass --output-style compressed src/styles/main.scss public/styles.css",
    "watch": "nodemon --ext css,eno,js,scss --ignore public --exec 'npm run build'"
