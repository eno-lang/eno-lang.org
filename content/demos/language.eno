- mkdir -p clang-build
- cd clang-build
- scan-build cmake .. -DCMAKE_BUILD_TYPE=Debug
- VERBOSE=1 scan-build -o ../scan make -j2
  
## artifacts
paths:
- scan/

## only
variables:
- $SCHEDULED_JOB_NAME == "clang"

